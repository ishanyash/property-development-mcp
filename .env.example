# API Keys - Replace with your actual API keys
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here
RIGHTMOVE_API_KEY=your_rightmove_api_key_if_available
LAND_REGISTRY_API_KEY=your_land_registry_api_key_if_available

# Rate Limiting Configuration
MAX_REQUESTS_PER_MINUTE=60
CACHE_DURATION_MINUTES=30

# Development Settings
NODE_ENV=development
LOG_LEVEL=debug

# Financial Assumptions
TARGET_PROFIT_MARGIN=0.25
FINANCE_RATE=0.14
ACQUISITION_COST_RATE=0.05
SELLING_COST_RATE=0.03

# API Endpoints (optional overrides)
POSTCODE_API_URL=https://api.postcodes.io
RIGHTMOVE_BASE_URL=https://www.rightmove.co.uk
LAND_REGISTRY_API_URL=https://landregistry.data.gov.uk

# Timeout settings (milliseconds)
API_TIMEOUT=30000
SCRAPING_TIMEOUT=15000