        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@types/jest": "^29.5.8",
        "@types/node": "^20.9.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8.54.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
