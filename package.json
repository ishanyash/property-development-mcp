    "version": "1.0.0",
    "description": "MCP server for property development analysis and reporting",
    "type": "module",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "tsx src/server.ts",
